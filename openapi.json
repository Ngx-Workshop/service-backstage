          "street": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "zip": {
            "type": "string"
          }
        },
        "required": [
          "street",
          "city",
          "state",
          "zip"
        ]
      },
      "CreateExampleMongodbDocDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "SOME_ENUM",
              "SOME_OTHER_ENUM"
            ]
          },
          "description": {
            "type": "string"
          },
          "archived": {
            "type": "boolean"
          },
          "exampleMongodbDocObject": {
            "$ref": "#/components/schemas/ExampleMongodbDocObjectDto"
          },
          "lastUpdated": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "name",
          "type"
        ]
      },
      "ExampleMongodbDocDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "SOME_ENUM",
              "SOME_OTHER_ENUM"
            ]
          },
          "version": {
            "type": "number"
          },
          "description": {
            "type": "string"
          },
          "lastUpdated": {
            "type": "string",
            "format": "date-time"
          },
          "archived": {
            "type": "boolean"
          },
          "exampleMongodbDocObject": {
            "$ref": "#/components/schemas/ExampleMongodbDocObjectDto"
          },
          "__v": {
            "type": "number"
          }
        },
        "required": [
          "_id",
          "name",
          "type",
          "version",
          "description",
          "lastUpdated",
          "archived",
          "__v"
        ]
      },
      "UpdateExampleMongodbDocDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "SOME_ENUM",
              "SOME_OTHER_ENUM"
            ]
          },
          "description": {
            "type": "string"
          },
          "archived": {
            "type": "boolean"
          },
          "exampleMongodbDocObject": {
            "$ref": "#/components/schemas/ExampleMongodbDocObjectDto"
          },
          "lastUpdated": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "DocBlobDto": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "description": "Raw content of the document"
          },
          "sha": {
            "type": "string",
            "description": "Git SHA of the fetched file"
          },
          "fetchedAt": {
            "type": "string",
            "format": "date-time"
          },
          "format": {
            "type": "string",
            "enum": [
              "yaml",
              "json",
              "markdown"
            ]
          }
        },
        "required": [
          "content"
        ]
      },
      "ServiceSummaryDto": {
        "type": "object",
        "properties": {
          "repoName": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "topics": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "defaultBranch": {
            "type": "string"
          },
          "htmlUrl": {
            "type": "string"
          },
          "lastSyncAt": {
            "type": "string",
            "format": "date-time"
          },
          "syncStatus": {
            "type": "string",
            "enum": [
              "idle",
              "ok",
              "partial",
              "failed",
              "rate_limited"
            ]
          },
          "syncError": {
            "type": "string"
          },
          "readme": {
            "$ref": "#/components/schemas/DocBlobDto"
          },
          "openapi": {
            "$ref": "#/components/schemas/DocBlobDto"
          },
          "runbook": {
            "$ref": "#/components/schemas/DocBlobDto"
          },
          "serviceMetadata": {
            "$ref": "#/components/schemas/DocBlobDto"
          }
        },
        "required": [
          "repoName",
          "topics",
          "syncStatus"
        ]
      },
      "ServiceDetailDto": {
        "type": "object",
        "properties": {
          "repoName": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "topics": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "defaultBranch": {
            "type": "string"
          },
          "htmlUrl": {
            "type": "string"
          },
          "lastSyncAt": {
            "type": "string",
            "format": "date-time"
          },
          "syncStatus": {
            "type": "string",
            "enum": [
              "idle",
              "ok",
              "partial",
              "failed",
              "rate_limited"
            ]
          },
          "syncError": {
            "type": "string"
          },
          "readme": {
            "$ref": "#/components/schemas/DocBlobDto"
          },
          "openapi": {
            "$ref": "#/components/schemas/DocBlobDto"
          },
          "runbook": {
            "$ref": "#/components/schemas/DocBlobDto"
          },
          "serviceMetadata": {
            "$ref": "#/components/schemas/DocBlobDto"
          }
        },
        "required": [
          "repoName",
          "topics",
          "syncStatus"
        ]
      },
      "SyncRequestDto": {
        "type": "object",
        "properties": {
          "repos": {
            "description": "Optional list of repo names to sync; defaults to all repos in the org",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "force": {
            "type": "boolean",
            "description": "Force re-fetch even if SHA is unchanged"
          }
        }
      },
      "SyncResponseDto": {
        "type": "object",
        "properties": {
          "total": {
            "type": "number"
          },
          "succeeded": {
            "type": "number"
          },
          "failed": {
            "type": "number"
          },
          "durationMs": {
            "type": "number",
            "description": "Duration in milliseconds"
          },
          "failures": {
            "description": "Repos that failed to sync (with error message inline if available)",
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "total",
          "succeeded",
          "failed",
          "durationMs",
          "failures"
        ]
      }
    }
  }
